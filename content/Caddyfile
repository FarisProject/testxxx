{
        log {
                exclude http.handlers.reverse_proxy
        }
}
:{$PORT}
encode gzip
# Configure tachidesk
handle /tachidesk* {
        redir /tachidesk /tachidesk/         
        uri strip_prefix /tachidesk
        reverse_proxy * localhost:4567
}
# Configure ttyd
handle /ttyd* {
        redir /ttyd /ttyd/
        uri strip_prefix /ttyd       
        reverse_proxy * localhost:7681
}